<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane prefWidth="600.0" style="-fx-background-color: black;" stylesheets="/main/css/Main.css" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.controller.VoteEditViewController">
   <children>
      <VBox id="tab-pane-wrapper" prefWidth="600.0">
         <padding>
            <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
         </padding>
         <children>
            <Label id="section" prefHeight="48.0" prefWidth="264.0" text="Vote edit" />
            <Separator prefHeight="1.0" prefWidth="854.0">
               <padding>
                  <Insets top="6.0" />
               </padding>
            </Separator>
            <Label layoutX="26.0" layoutY="156.0" text="Ballot" textFill="WHITE">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Label>
            <TableView fx:id="ballotTableView" prefHeight="200.0" prefWidth="200.0">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
               <columns>
                  <TableColumn fx:id="alternativeColumn" prefWidth="75.0" text="Alternative" />
                  <TableColumn fx:id="rankingColumn" prefWidth="75.0" text="Ranking" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <Label layoutX="26.0" layoutY="156.0" text="Categories" textFill="WHITE">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Label>
            <TableView fx:id="categoryTableView" prefHeight="200.0" prefWidth="200.0">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
               <columns>
                  <TableColumn fx:id="familyColumn" prefWidth="75.0" text="Family" />
                  <TableColumn fx:id="categoryColumn" prefWidth="75.0" text="Category" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
         <StackPane.margin>
            <Insets />
         </StackPane.margin>
      </VBox>
   </children>
</StackPane>
