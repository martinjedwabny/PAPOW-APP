<StackPane fx:id="mainPane" maxHeight="1.7976931348623157E308" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.controller.InputTabViewController">
   <children>
      <AnchorPane stylesheets="/main/css/Main.css">
         <children>
            <ScrollPane id="tab-pane-wrapper" fitToWidth="true" layoutX="10.0" layoutY="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <content>
                  <VBox id="tab-pane-wrapper">
                     <children>
                        <HBox>
                           <children>
                              <VBox prefWidth="350.0" HBox.hgrow="ALWAYS">
                                 <children>
                                    <Label id="section" prefHeight="48.0" prefWidth="264.0" text="Voters" />
                                    <Separator prefHeight="1.0">
                                       <padding>
                                          <Insets top="6.0" />
                                       </padding>
                                    </Separator>
                                    <Label id="section-description" layoutX="10.0" layoutY="10.0" text="Double-click to edit. Add and delete with buttons below.">
                                       <VBox.margin>
                                          <Insets top="6.0" />
                                       </VBox.margin>
                                    </Label>
                                    <TableView fx:id="votersTableView" editable="true">
                                       <columnResizePolicy>
                                          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                       </columnResizePolicy>
                                       <columns>
                                          <TableColumn fx:id="voterNameColumn" prefWidth="75.0" text="Name" />
                                       </columns>
                                       <VBox.margin>
                                          <Insets top="10.0" />
                                       </VBox.margin>
                                    </TableView>
                                    <HBox alignment="BOTTOM_RIGHT" spacing="10.0">
                                       <children>
                                          <JFXButton onMouseClicked="#addVoter" text="Add" />
                                          <JFXButton onMouseClicked="#deleteVoter" text="Delete" />
                                       </children>
                                       <opaqueInsets>
                                          <Insets />
                                       </opaqueInsets>
                                       <VBox.margin>
                                          <Insets top="10.0" />
                                       </VBox.margin>
                                    </HBox>
                                 </children>
                                 <HBox.margin>
                                    <Insets />
                                 </HBox.margin>
                              </VBox>
                              <VBox prefWidth="350.0" HBox.hgrow="ALWAYS">
                                 <children>
                                    <Label id="section" prefHeight="48.0" prefWidth="264.0" text="Alternatives" />
                                    <Separator prefHeight="1.0">
                                       <padding>
                                          <Insets top="6.0" />
                                       </padding>
                                    </Separator>
                                    <Label id="section-description" text="Double-click to edit. Add and delete with buttons below.">
                                       <VBox.margin>
                                          <Insets top="6.0" />
                                       </VBox.margin>
                                    </Label>
                                    <TableView fx:id="alternativesTableView" editable="true">
                                       <columns>
                                          <TableColumn fx:id="alternativeNameColumn" prefWidth="75.0" text="Name" />
                                       </columns>
                                       <columnResizePolicy>
                                          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                       </columnResizePolicy>
                                       <VBox.margin>
                                          <Insets top="10.0" />
                                       </VBox.margin>
                                    </TableView>
                                    <HBox alignment="BOTTOM_RIGHT" spacing="10.0">
                                       <children>
                                          <JFXButton onMouseClicked="#addAlternative" text="Add" />
                                          <JFXButton onMouseClicked="#deleteAlternative" text="Delete" />
                                       </children>
                                       <opaqueInsets>
                                          <Insets />
                                       </opaqueInsets>
                                       <VBox.margin>
                                          <Insets top="10.0" />
                                       </VBox.margin>
                                    </HBox>
                                 </children>
                                 <HBox.margin>
                                    <Insets left="27.0" />
                                 </HBox.margin>
                              </VBox>
                              <VBox layoutX="450.0" layoutY="10.0" prefWidth="500.0">
                                 <children>
                                    <Label id="section" prefHeight="48.0" prefWidth="264.0" text="Categories" />
                                    <Separator prefHeight="1.0">
                                       <padding>
                                          <Insets top="6.0" />
                                       </padding>
                                    </Separator>
                                    <Label id="section-description" text="Double-click to edit. Add and delete with buttons below.">
                                       <VBox.margin>
                                          <Insets top="6.0" />
                                       </VBox.margin>
                                    </Label>
                                    <JFXTreeTableView fx:id="categoryTreeTableView" editable="true">
                                       <columns>
                                          <JFXTreeTableColumn fx:id="categoryFamilyColumn" prefWidth="75.0" text="Family" />
                                          <JFXTreeTableColumn fx:id="categoryNameColumn" prefWidth="75.0" text="Category" />
                                       </columns>
                                       <columnResizePolicy>
                                          <TreeTableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                       </columnResizePolicy>
                                       <VBox.margin>
                                          <Insets top="10.0" />
                                       </VBox.margin>
                                    </JFXTreeTableView>
                                    <HBox alignment="BOTTOM_RIGHT" spacing="10.0">
                                       <children>
                                          <TextField fx:id="categoryFamilyTextField" prefWidth="120.0" promptText="Family" style="-fx-background-color: #272727; -fx-border-color: #747474;" />
                                          <TextField fx:id="categoryNameTextField" prefWidth="120.0" promptText="Category" style="-fx-background-color: #272727; -fx-border-color: #747474;" />
                                          <JFXButton onMouseClicked="#addCategory" text="Add" />
                                          <JFXButton onMouseClicked="#deleteCategory" text="Delete" />
                                       </children>
                                       <opaqueInsets>
                                          <Insets />
                                       </opaqueInsets>
                                       <VBox.margin>
                                          <Insets top="10.0" />
                                       </VBox.margin>
                                    </HBox>
                                 </children>
                                 <HBox.margin>
                                    <Insets left="27.0" />
                                 </HBox.margin>
                              </VBox>
                           </children>
                        </HBox>
                        <Label id="section" prefHeight="48.0" prefWidth="264.0" text="Questions">
                           <VBox.margin>
                              <Insets top="40.0" />
                           </VBox.margin>
                        </Label>
                        <Separator prefHeight="1.0" prefWidth="854.0">
                           <padding>
                              <Insets top="6.0" />
                           </padding>
                        </Separator>
                        <Label id="section-description" text="Open edit popup window by double clicking. Add and delete with buttons below.">
                           <VBox.margin>
                              <Insets top="6.0" />
                           </VBox.margin>
                        </Label>
                        <TableView fx:id="questionsTableView" editable="true">
                           <VBox.margin>
                              <Insets top="10.0" />
                           </VBox.margin>
                           <columns>
                              <TableColumn fx:id="questionDescriptionColumn" editable="false" prefWidth="75.0" text="Description" />
                              <TableColumn fx:id="questionAlternativesColumn" editable="false" prefWidth="75.0" text="Alternatives" />
                              <TableColumn fx:id="questionVotesColumn" editable="false" prefWidth="75.0" text="Votes" />
                           </columns>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                        </TableView>
                        <HBox alignment="BOTTOM_RIGHT" spacing="10.0">
                           <children>
                              <JFXButton onMouseClicked="#addQuestion" text="Add" />
                              <JFXButton onMouseClicked="#deleteQuestion" text="Delete" />
                           </children>
                           <opaqueInsets>
                              <Insets />
                           </opaqueInsets>
                           <VBox.margin>
                              <Insets top="10.0" />
                           </VBox.margin>
                        </HBox>
                     </children>
                     <padding>
                        <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
                     </padding>
                  </VBox>
               </content>
            </ScrollPane>
         </children>
      </AnchorPane>
   </children>
